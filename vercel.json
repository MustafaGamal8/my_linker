{
  "version": 2,
  "builds": [
    {
      "src": "./client",
      "use": "@vercel/node"
    },
    {
      "src": "./index.js",  
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./client/index.html"  
    }
  ]
}
